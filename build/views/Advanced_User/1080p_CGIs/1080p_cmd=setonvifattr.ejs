<% include ../../_partials/template/_head.ejs %><body> <% include ../../_partials/template/_outdated.ejs %><div id="wrapper"><header><% include ../../_partials/template/_header.ejs %></header><div class="container mb"><h1>1080p CGI Commands - Set ONVIF Attribute</h1><hr></div><div class="clearfix"></div> <!-- #############################################  Body ############################################# --><div class="container main"><section class="row" style="margin-top: 15px;"><div class="col-12"><h2>Network :: Set ONVIF Attribute</h2><h3>param.cgi?cmd=setonvifattr</h3><div class="table-responsive"><table class="table table-hover"><tr><th>Function</th><td>Set ONVIF Parameter</td></tr><tr><th>Authority</th><td>admin</td></tr><tr><th>Syntax</th><td><code>/cgi-bin/hi3510/param.cgi[?cmd=setonvifattr&amp;-ov_enable=&amp;-ov_port=&amp;ov_authflag=]</code></td></tr><tr><th rowspan="3">Parameter</th><td>ov_enable: 1 (ONVIF enabled), 0 (ONVIF disabled)</td></tr><tr><td>ov_port: ONVIF Port (8080-65536)</td></tr><tr><td>ov_authflag: Authentification 1 (on), 0 (off)</td></tr><tr><th>Return</th><td>Success, Error</td></tr><tr><th>Remarks</th><td>Modified successfully, the device will automatically restart</td></tr><tr><th>Example</th><td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=setonvifattr&-ov_enable=1&-ov_port=8080&amp;ov_authflag=0</code></td></tr></table></div></div></section><div class="clearfix" style="margin-bottom: 50px;"></div></div><section class="webui mb"><% include ../../_partials/content/_webui.ejs %></section><section class="social"><% include ../../_partials/template/_social.ejs %></section> <!-- ############################################# Footer ############################################# --><footer><% include ../../_partials/template/_footer.ejs %></footer> <% include ../../_partials/template/_jsdefaults.ejs %></div></body></html>