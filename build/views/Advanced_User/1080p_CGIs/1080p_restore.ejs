<% include ../../_partials/template/_head.ejs %><body> <% include ../../_partials/template/_outdated.ejs %><div id="wrapper"><header><% include ../../_partials/template/_header.ejs %></header><div class="container mb"><h1>1080p CGI Commands - System Restore</h1><hr></div><div class="clearfix"></div> <!-- #############################################  Body ############################################# --><div class="container main"><section class="row" style="margin-top: 15px;"><div class="col-12"><h2>System :: Restore</h2><h3>restore.cgi</h3><div class="table-responsive"><table class="table table-hover"><tr><th>Function</th><td>Restore to settings from a backup file</td></tr><tr><th>Authority</th><td>admin</td></tr><tr><th>Syntax</th><td><code>/cgi-bin/hi3510/restore.cgi[?-setting_file=]</code></td></tr><tr><th>Parameter</th><td>setting_file: System set file path</td></tr><tr><th>Remarks</th><td>This cgi must used as a POST. The update file is then send to the IP address.</td></tr><tr><th>Example</th><td><code>&lt;form name="form6" method="post" enctype="multipart/form-data" action="cgi-bin/hi3510/restore.cgi"&gt;<br/>&lt;input type="file" name="setting_file"&gt;<br/>&lt;input type="submit" value="restore"&gt;<br/>&lt;/form&gt;</code></td></tr></table></div></div></section><div class="clearfix" style="margin-bottom: 50px;"></div></div><section class="webui mb"><% include ../../_partials/content/_webui.ejs %></section><section class="social"><% include ../../_partials/content/_social.ejs %></section> <!-- ############################################# Footer ############################################# --><footer><% include ../../_partials/template/_footer.ejs %></footer> <% include ../../_partials/template/_jsdefaults.ejs %></div></body></html>