<body>
    <% include ../../_partials/template/_outdated.ejs %>

    <div id="wrapper">

        <header><% include ../../_partials/template/_header.ejs %></header>
        <!-- Navbar-->

            <div class="container mb">
                <h1>1080p CGI Commands - Get Server Time</h1>
                <hr>
            </div>

            <div class="clearfix"></div>

            <div class="container main">

              <!-- ######################################## Body ####################################### -->

              <section class="row" style="margin-top: 15px;">
                <!-- Media Container -->

                <div class="col-12">

                  <h2>System :: Get Server Time</h2>
                  <h3>param.cgi?cmd=getservertime</h3>

                  <div class="table-responsive">
                    <table class="table table-hover">
                      <tr>
                        <th>Function</th>
                        <td>Get server time</td>
                      </tr>
                      <tr>
                        <th>Authority</th>
                        <td>admin</td>
                      </tr>
                      <tr>
                        <th>Syntax</th>
                        <td><code>/cgi-bin/hi3510/param.cgi[?cmd=getservertime]</code></td>
                      </tr>
                      <tr>
                        <th rowspan="3">Return</th>
                        <td>time:<br/>Current time - [yyyy][mm][dd][hh][mm][ss]<br/>E.g. time="20110802182638";</td>
                      </tr>
                      <tr>
                        <td>timeZone: e.g. timeZone="Asia/Hong_Kong";</td>
                      </tr>
                      <tr>
                        <td>dstmode: set summer time enable (on) disable (off)</td>
                      </tr>
                      <tr>
                        <th>Example</th>
                        <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=getservertime</code></td>
                      </tr>
                    </table>
                  </div>

                </div>

                </section>  <!-- Close Media Container -->

                <div class="clearfix" style="margin-bottom: 50px;"></div>

          </div>

