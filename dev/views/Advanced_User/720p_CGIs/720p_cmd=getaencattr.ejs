    <% include ../../_partials/template/_outdated.ejs %>

    <div id="wrapper">

        <header><% include ../../_partials/template/_header.ejs %></header>
        <!-- Navbar-->

            <div class="container mb">
                <h1>720p CGI Commands - Get Audio Encoder Attribute</h1>
                <hr>
            </div>

            <div class="clearfix"></div>

            <div class="container main">

              <!-- ######################################## Body ####################################### -->

              <section class="row" style="margin-top: 15px;">
                <!-- Media Container -->

                <div class="col-12">

                  <h2>Video &amp; Audio :: Get Audio Encoder Attribute</h2>
                  <h3>param.cgi?cmd=getaencattr</h3>

                  <div class="table-responsive">
                    <table class="table table-hover">
                      <tr>
                        <th>Function</th>
                        <td>Get Audio Encoder Parameter</td>
                      </tr>
                      <tr>
                        <th>Authority</th>
                        <td>admin</td>
                      </tr>
                      <tr>
                        <th>Syntax</th>
                        <td><code>/cgi-bin/hi3510/param.cgi[?cmd=getaencattr&amp;-chn=]</code></td>
                      </tr>
                      <tr>
                        <th>Parameter</th>
                        <td>chn:<br/>11: first bit rate<br/>12ï¼šSecond bit rate<br/>13: third bit rate</td>
                      </tr>
                      <tr>
                        <th rowspan="2">Return</th>
                        <td>aeswitch_1, aeswitch_2 or aeswitch_3<br/>Audio Encoder Switch<br/>0: switch off, 1: switch on</td>
                      </tr>
                      <tr>
                        <td>aeformat_1, aeformat_2 or aeformat_3<br/>Audio Encoder Format<br/>g711a: G711A 64Kbps, g726: G726 16Kbps</td>
                      </tr>
                      <tr>
                        <th>Remarks</th>
                        <td>_1 first bit rate, _2 second bit rate ,_3 3rd bit rate<br/>Audio sampling: 8K, mono: 16 bits wide</td>
                      </tr>
                      <tr>
                        <th>Example</th>
                        <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=getaencattr&-chn=11</code></td>
                      </tr>
                    </table>
                  </div>

                </div>

                </section>  <!-- Close Media Container -->

