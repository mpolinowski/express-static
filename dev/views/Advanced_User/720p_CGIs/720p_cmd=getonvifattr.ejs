              <!-- ######################################## Body ####################################### -->

              <section class="row" style="margin-top: 15px;">
                <!-- Media Container -->

                <div class="col-12">

                  <h2>Network :: Get ONVIF Attribute</h2>
                  <h3>param.cgi?cmd=getonvifattr</h3>

                  <div class="table-responsive">
                    <table class="table table-hover">
                      <tr>
                        <th>Function</th>
                        <td>Get ONVIF Parameter</td>
                      </tr>
                      <tr>
                        <th>Authority</th>
                        <td>admin</td>
                      </tr>
                      <tr>
                        <th>Syntax</th>
                        <td><code>/cgi-bin/hi3510/param.cgi[?cmd=getonvifattr]</code></td>
                      </tr>
                      <tr>
                        <th rowspan="3">Return</th>
                        <td>ov_enable: 1 (on), 0 (off)</td>
                      </tr>
                      <tr>
                        <td>ov_port: ONVIF Port (8080-65536)</td>
                      </tr>
                      <tr>
                        <td>ov_authflag: Authentification 1 (on), 0 (off)</td>
                      </tr>
                      <tr>
                        <th>Example</th>
                        <td><code>http://admin:instar@192.168.178.88/cgi-bin/hi3510/param.cgi?cmd=getonvifattr</code></td>
                      </tr>
                    </table>
